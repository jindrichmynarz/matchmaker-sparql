{{!
@param IRI evaluation-graph
@param int limit
@param int offset
}}

{{> templates/prefixes.mustache}}

SELECT ?resource ?match
WHERE {
  {
    SELECT ?resource ?match
    WHERE {
      GRAPH <{{evaluation-graph}}> {
        ?resource pc:awardedTender/pc:bidder ?match .
      }
    }
    ORDER BY SHA1(?resource) {{! FIXME: Is there an implicit temporal order in resource IRIs? }}
  }
}
LIMIT {{limit}}
OFFSET {{offset}}
